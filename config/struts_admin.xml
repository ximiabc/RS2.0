<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<!-- 高级管理员 -->
	<package name="admin" extends="struts-default" namespace="/">
		<!--  -->
		<action name="admin_*" class="com.zqrc.rs.pro.action.UserAction" method="{1}">
			<!-- 通用操作************ -->
			<!-- 欢迎面板 -->
			<result name="main">/WEB-INF/view/main.jsp</result>
			<!-- 帮助界面 -->
			<result name="help">/WEB-INF/view/help.jsp</result>
			
			<!-- 个人操作************ -->
			<!-- 个人管理页 -->
			<result name="person">/WEB-INF/view/admin/personal.jsp</result>
			
			<!-- 管理员操作 **********-->
			<!-- 浏览管理员 -->
			<result name="manageList">/WEB-INF/view/manage/list_view.jsp</result>
			
			<!-- 学校操作************ -->
			<!-- 浏览学校 -->
			<result name="schoolList">/WEB-INF/view/master/list_view.jsp</result>
			
			<!-- 教师操作************ -->
			<!-- 教师浏览 -->
			<result name="teacherList">/WEB-INF/view/teacher/list_view.jsp</result>
			
			<!-- 报名管理************ -->
			<!-- 报名日期 -->
			<result name="rs_date">/WEB-INF/view/admin/rs_date.jsp</result>
			<!-- 报名字段 -->
			<result name="rs_key">/WEB-INF/view/admin/rs_key.jsp</result>
			
			<!-- 权限管理************ -->
			<!-- 教委权限 -->
			<result name="authority_manage">/WEB-INF/view/admin/authority_manage.jsp</result>
			<!-- 学校权限 -->
			<result name="authority_master">/WEB-INF/view/admin/authority_master.jsp</result>
			<!-- 教师权限 -->
			<result name="authority_teacher">/WEB-INF/view/admin/authority_teacher.jsp</result>
			
			<!-- 学生相关************ -->
			<!-- 导入报表 -->
			<result name="addExcel">/WEB-INF/view/admin/addExcel.jsp</result>
			<!-- 学生浏览 -->
			<result name="listStudent">/WEB-INF/view/admin/student_list.jsp</result>
			<!-- 学生审核 -->
			<result name="auditStudent">/WEB-INF/view/admin/student_audit.jsp</result>
		</action>
	</package>
</struts>
